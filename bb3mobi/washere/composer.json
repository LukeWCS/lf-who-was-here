{
    "name": "bb3mobi/washere",
    "type": "phpbb-extension",
    "description": "Shows a visitor statistic of the current day or a freely selectable period. It lists both visitors and bots, as well as numbers on visible members, invisible members, bots and guests. Furthermore, the extension offers a visitor record.",
    "homepage": "https://www.phpbb.de/community/viewtopic.php?f=149&t=241976",
    "version": "1.3.2",
    "time": "2018-09-23",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Joas Schilling [nickvergessen]",
            "homepage": "http://www.flying-bits.org/",
            "role": "Developer"
        },
        {
            "name": "Anvar",
            "homepage": "http://phpbbguru.net",
            "role": "Converting to extension"
        },
        {
            "name": "LukeWCS",
            "homepage": "https://www.wcsaga.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
      "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "LF who was here",
        "soft-require": {
        "phpbb/phpbb": ">=3.2"
        },
		"version-check": {
			"host": "raw.githubusercontent.com",
			"directory": "/LukeWCS/lf-who-was-here/master",
			"filename": "lf-who-was-here_version.json",
			"ssl": true
		}
    }
}
